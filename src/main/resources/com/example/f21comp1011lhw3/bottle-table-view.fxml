<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.f21comp1011lhw3.Controllers.BottleTableViewController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="tableView" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="bottleIdColumn" prefWidth="102.0" text="Bottle ID" />
                <TableColumn fx:id="companyColumn" minWidth="20.0" prefWidth="100.0" text="Company" />
                  <TableColumn fx:id="brandColumn" minWidth="20.0" prefWidth="100.0" text="Brand" />
                  <TableColumn fx:id="scentedColumn" minWidth="20.0" prefWidth="90.0" text="Scented" />
                  <TableColumn fx:id="volumeColumn" minWidth="20.0" prefWidth="90.0" text="Volume" />
                  <TableColumn fx:id="alcoholColumn" minWidth="20.0" prefWidth="110.0" text="Alcohol %" />
                  <TableColumn fx:id="pumpTopColumn" minWidth="20.0" prefWidth="114.0" text="Pump Top" />
                  <TableColumn fx:id="refillableColumn" minWidth="20.0" prefWidth="98.0" text="Refillable" />
                  <TableColumn fx:id="unitsSoldColumn" minWidth="20.0" prefWidth="93.0" text="Units Sold" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <bottom>
            <Button mnemonicParsing="false" text="Chart View" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
